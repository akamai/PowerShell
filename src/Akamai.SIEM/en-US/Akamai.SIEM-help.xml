    <command:details>
      <command:name>Format-SIEMEvent</command:name>
      <command:verb>Format</command:verb>
      <command:noun>SIEMEvent</command:noun>
      <maml:description>
        <maml:para>Format and decode a SIEM event object.</maml:para>
      </maml:description>
    </command:details>
